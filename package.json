{
  "name": "annotationlib",
  "version": "1.0.0",
  "description": "YouTube annotation parser and renderer",
  "main": "index.js",
  "scripts": {
    "build:parser": "parcel build src/parser/index.js --out-dir dist --out-file AnnotationParser.js --no-source-maps",
    "build:renderer": "parcel build src/renderer/index.js --out-dir dist --out-file AnnotationRenderer.js --no-source-maps",
    "build": "npm run build:parser & npm run build:renderer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afrmtbl/annotationlib.git"
  },
  "keywords": [
    "youtube",
    "annotations"
  ],
